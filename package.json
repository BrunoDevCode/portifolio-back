    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
